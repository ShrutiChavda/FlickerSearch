{
pcode = pcd;
pname = pnm;
mname = mnm;
}
public void Display()
{
Console.WriteLine("\nProduct Code:= " + pcode);
Console.WriteLine("\nProduct Name:= " + pname);
Console.WriteLine("\nManufacturer Name:= " + mname);
}

 
}
